{
  "expo": {
    "name": "sdm-app",
    "slug": "sdm-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to provide accurate pickup and drop-off services.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to provide accurate pickup and drop-off services."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyAejqe2t4TAptcLnkpoFTTNMhm0SFHFJgQ"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAejqe2t4TAptcLnkpoFTTNMhm0SFHFJgQ"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
